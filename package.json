{
  "name": "app-manifest-loader",
  "version": "1.0.0",
  "description": "Web App Manifest Loader for Webpack",
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "prepack": "preppy",
    "test": "preppy && jest",
    "release": "git push && release-it --github.release --npm.publish --non-interactive",
    "release:minor": "git push && release-it --github.release --npm.publish --non-interactive --increment minor",
    "release:major": "git push && release-it --github.release --npm.publish --non-interactive --increment major"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/markdalgleish/app-manifest-loader.git"
  },
  "author": {
    "name": "Sebastian Software",
    "email": "s.werner@sebastian-software.de",
    "url": "https://www.sebastian-software.de"
  },
  "engines": {
    "node": ">=6.0.0",
    "yarn": ">=0.21.3",
    "npm": ">=4.0.0"
  },
  "pre-commit": "lint-staged",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/markdalgleish/app-manifest-loader/issues"
  },
  "homepage": "https://github.com/markdalgleish/app-manifest-loader#readme",
  "jest": {
    "testRegex": "test/index.js",
    "collectCoverageFrom": [
      "src/index.js"
    ]
  },
  "dependencies": {
    "steed": "^1.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "babel-core": "^7.0.0-bridge.0",
    "babel-preset-edge": "^3.3.0",
    "eslint-config-readable": "^1.11.1",
    "file-loader": "^1.1.11",
    "glob": "^7.1.2",
    "jest": "^22.4.3",
    "preppy": "^4.2.2",
    "prettier": "^1.12.1",
    "rimraf": "^2.6.2",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.2"
  }
}
